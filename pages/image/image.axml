<view class="page">
  <view class="page-description">图片<view>
  <view class="page-section" a:for="{{models}}" a:for-item="item">
    <view class="page-section-title">text</view>
    <view class="page-section-demo">
      <image class="image"
        data-name="{{item.mode}}"
        onTap="onTap"
        mode="{{item.mode}}" src="{{src}}" onError="imageError" onLoad="imageLoad"/>
    </view>
  </view>
</view>